{
  "manifest_version": 3,
  "name": "GitHub Redirect",
  "version": "1.0",
  "description": "Redirects GitHub URLs based on API response",
  "permissions": [
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "https://github.com/*",
    "https://github.lucasvanbriemen.nl/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
